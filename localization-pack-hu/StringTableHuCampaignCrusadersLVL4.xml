        <LocalizedStrings>
            <!--OBJECTIVES MAIN QUESTS-->
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_START_LVL4</LocID>                      <Text>Lure the UNSS fleet</Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_CRUS_BASES_1</LocID>               <Text>Occupy the nearby bases 0/4</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_CRUS_BASES_2</LocID>               <Text>Occupy the nearby bases 1/4</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_CRUS_BASES_3</LocID>               <Text>Occupy the nearby bases 2/4</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_CRUS_BASES_4</LocID>               <Text>Occupy the nearby bases 3/4</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_CRUS_BASES_DONE</LocID>            <Text><![CDATA[<color=SOTS_COLOR_MOVEMENT>Occupy the nearby bases 4/4 - Finished</color>]]></Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_STASH_1</LocID>                    <Text>Get resources 0/3</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_STASH_2</LocID>                    <Text>Get resources 1/3</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_STASH_3</LocID>                    <Text>Get resources 2/3</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_STASH_DONE</LocID>                 <Text><![CDATA[<color=SOTS_COLOR_MOVEMENT>Get resources 3/3 - Finished</color>]]></Text>  </GameDBLocalizedString>


            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_CROSS_BRIDGE</LocID>               <Text>Cross the bridge</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_CROSS_BRIDGE_DONE</LocID>          <Text><![CDATA[<color=SOTS_COLOR_MOVEMENT>Cross the bridge - Finished</color>]]></Text>  </GameDBLocalizedString>
            
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_REPAIR_TELEPORT</LocID>            <Text><![CDATA[Repair the teleport <color=SOTS_COLOR_RED>with Princess</color>]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_REPAIR_TELEPORT_DONE</LocID>       <Text><![CDATA[<color=SOTS_COLOR_MOVEMENT>Repair the teleport with Princess - Finished</color>]]></Text>  </GameDBLocalizedString>
            
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_OPEN_BRIDGE</LocID>                <Text><![CDATA[Open the bridge <color=SOTS_COLOR_RED>with Princess</color>]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_OPEN_BRIDGE_DONE</LocID>           <Text><![CDATA[<color=SOTS_COLOR_MOVEMENT>Open the bridge with Princess - Finished</color>]]></Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_SF_BASES_1</LocID>                 <Text>Seize mercenaries' bases 0/2</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_SF_BASES_2</LocID>                 <Text>Seize mercenaries' bases 1/2</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_SF_BASES_DONE</LocID>              <Text><![CDATA[<color=SOTS_COLOR_MOVEMENT>Seize mercenaries' bases 2/2 - Finished</color>]]></Text>  </GameDBLocalizedString>
           
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_SF_MERCHS_1</LocID>                <Text>Defeat the mercenaries 0/2</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_SF_MERCHS_2</LocID>                <Text>Defeat the mercenaries 1/2</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_SF_MERCHS_DONE</LocID>             <Text><![CDATA[<color=SOTS_COLOR_MOVEMENT>Defeat the mercenaries 2/2 - Finished</color>]]></Text>  </GameDBLocalizedString>
  
          
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_RAM_GATE</LocID>                   <Text><![CDATA[Ram the gate <color=SOTS_COLOR_RED>with Princess</color>]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_RAM_GATE_DONE</LocID>              <Text><![CDATA[<color=SOTS_COLOR_MOVEMENT>Ram the gate with Princess - Finished</color>]]></Text>  </GameDBLocalizedString>
            
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASES_1</LocID>               <Text>Capture the UNSS bases 0/4</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASES_2</LocID>               <Text>Capture the UNSS bases 1/4</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASES_3</LocID>               <Text>Capture the UNSS bases 2/4</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASES_4</LocID>               <Text>Capture the UNSS bases 3/4</Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_GET_TO_SHIP</LocID>                <Text><![CDATA[<color=SOTS_COLOR_RED>Get Princess to the ship Bright Star - remaining turn(s):</color>]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_GET_TO_SHIP_DONE</LocID>           <Text><![CDATA[<color=SOTS_COLOR_MOVEMENT>Get Princess to the ship Bright Star - Finished</color>]]></Text>  </GameDBLocalizedString>
            
            <!--DIALOGUES-->
            <!--Heroes arrived to the orbit of planet Sirena, on the surface there is main base Princess evil sister. They are standing next to ancient battleship Bright Star.   -->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_NARRATOR_1</LocID>             <Text>Amids the asteroid field of the Siren, the 4th planet of the System. </Text>  </GameDBLocalizedString>
            
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_VIKTAR_1</LocID>               <Text>Very funny! Now, could the Princess be so kind and tell us the real plan? </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_PRINCESS_1</LocID>             <Text>I was not joking, Viki.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_VIKTAR_2</LocID>               <Text>Then I wish Princes was! We lure the UNSS fleet here, we get obliterated from orbit. It's as simple as that! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_PRINCESS_2</LocID>             <Text>That won't happen! Once the UNSS ships are here, I will call Bright Star ...</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_ANGAEDE_1</LocID>              <Text>... AND I WILL USE MY CONNECTION WITH UNSS SHIPS TO BROADCAST THE PRINCESS' VOICE BEARING THE WORDS OF LITANY.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_TIN_1</LocID>                  <Text>With the UNSS fleet in our hands, we can make them taste their own medicine! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_PRINCESS_2.5</LocID>           <Text>I'm pretty sure even my sister cannot survive orbital bombardment. </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_GILEAS_1</LocID>               <Text>But why act so soon? The Words of Bright Star convict the Grand Zieregal of the crimes he was blaming on the Princess and other clones of the Allmother. When the Children hear the truth, they will come to join just as I did. </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_PRINCESS_3</LocID>             <Text>When my sister learns about Bright Star, she will try to steal her the same way we plan to steal the UNSS fleet. We must attack now! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_VIKTAR_3</LocID>               <Text>So why, Princess, have you let us disembark in orbit?   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_TIN_2</LocID>                  <Text>During the Holy War, even the orbit was a battlefield, and after all that time, the bases are still here so we get some time to prepare. Once we advance to the ground, we'll be discovered.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_VIKTAR_4</LocID>               <Text>Right, I give up ... for now. </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_PRINCESS_4</LocID>             <Text>Let's go!</Text>  </GameDBLocalizedString>
          
            <!--After the dialog, everybody is teleported to their bases, Princess turns around and deactivates the bridge leading to Bright Star orbital islet.--> 
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_PRINCESS_5</LocID>             <Text>Just in case.</Text>  </GameDBLocalizedString> 
           
            <!--All Childrens bases are captured, now the heroes focus on gathering resources and exploration. Some production building are missing in orbit though. -->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_CRUS_BASES_PRINCESS_1</LocID>        <Text>Good, that's all of them.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_CRUS_BASES_TIN_1</LocID>             <Text>Some resources cannot be produced up here. Maybe we will be forced to move down much sooner than we expected.  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_CRUS_BASES_PRINCESS_2</LocID>        <Text>I will open the teleport down, but not before the scoop scrapes the bottom. There are some shipwrecks near ... </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_CRUS_BASES_GILEAS_1</LocID>          <Text>So we are looting now?  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_CRUS_BASES_VIKTAR_1</LocID>          <Text>Pull the stick from your rectum, human. The dead need no cargo. </Text>  </GameDBLocalizedString>

             <!--Time to repair the teleport down.-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_REPAIR_TELEPORT_RH_1</LocID>         <Text>All bigger stashes of resources have been depleted. Time to open the teleport down.  </Text>  </GameDBLocalizedString>
        
            <!-- Teleport is repaired.-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_TELEPORT_REPAIRED_PRINCESS_1</LocID> <Text>The way down is clear.</Text>  </GameDBLocalizedString>
            
            <!--  Princes, has used the teleport and descended to the ground.-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_ON_THE_GROUND_PRINCESS_1</LocID>     <Text>I am down on the surface!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_ON_THE_GROUND_GILEAS_1</LocID>       <Text>Can you use that psychosphere "thing" of yours to scout ahead?  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_ON_THE_GROUND_PRINCESS_2</LocID>     <Text>No, it could warn my Sister.  </Text>  </GameDBLocalizedString>

            <!--Part of the map is separated by the landslide, but the road suggest the area was passable once.-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_LAND_SLIDE_RH_1</LocID>              <Text>The road has been blocked by a landslide. An explosion could clear that away, but that would equal blowing a battle horn!  </Text>  </GameDBLocalizedString>

            <!--Hero encounters first UNSS commander. The UNSS commander runs to the near bridge and cross to avoid confrontation.-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GAHIJI_REPORT_GAHIJI_1</LocID>       <Text>Captain Rao! We have guests!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GAHIJI_REPORT_RAO_1</LocID>          <Text>Over the bridge and send mercenaries to secure it! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GAHIJI_REPORT_GAHIJI_2</LocID>       <Text>Roger that!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GAHIJI_REPORT_RH_1</LocID>           <Text>So it begins.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GAHIJI_REPORT_PRINCESS_1</LocID>     <Text>On the bright side, no point in avoiding the psychosphere anymore. </Text>  </GameDBLocalizedString>
            
           <!-- Hero get close to the now deactivated bridge and overhears the conversation of enemy commanders. Gahiji reports he spotted enemy to Captain Rao and Dr. Brown 9B. -->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_BRIDGE_DIALOG_GAHIJI_1</LocID>       <Text>... not far away from here.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_BRIDGE_DIALOG_RAO_1</LocID>          <Text>I see... good job turning the bridge off, Commander Gahiji. We cannot afford to underestimate them this time.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_BRIDGE_DIALOG_BROWN_1</LocID>        <Text>What an ugly blade in your voice, Rao! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_BRIDGE_DIALOG_RAO_2</LocID>          <Text>Intended and well deserved, 9B! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_BRIDGE_DIALOG_BROWN_2</LocID>        <Text>Gergie-larh, you know what, you damn pirate?! ... You are right, I have screwed it up ... Now, who will inform Her?  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_BRIDGE_DIALOG_RAO_3</LocID>          <Text>I will do it. She would bite your head off if you bring her any more bad news. </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_BRIDGE_DIALOG_BROWN_3</LocID>        <Text>I guess She would... thank you, Rao ... </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_BRIDGE_DIALOG_RAO_4</LocID>          <Text>There is one more thing to do. Let's call Furret.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_BRIDGE_DIALOG_RAO_5</LocID>          <Text>Supreme Leader, any progress on blowing up those collapsed rocks blocking the east road?   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_BRIDGE_DIALOG_FURRET_1</LocID>       <Text>Such a thing cannot be done in a hurry, captain. A single mistake and we may deal with an even bigger collapse. Be patient!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_BRIDGE_DIALOG_TIN_1</LocID>          <Text>Furret is also here?! That treacherous bastard is trying to get to our rear!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_BRIDGE_DIALOG_PRINCESS_1</LocID>     <Text>We ought to prepare for that alternative too.</Text>  </GameDBLocalizedString>
            
            <!-- Princess uses her abilities to activate the bridge. -->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_ACTIVATE_BRIDGE_PRINCESS_1</LocID>   <Text>Deactivated bridges are my speciality!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_ACTIVATE_BRIDGE_PRINCESS_2</LocID>   <Text>Peace of cake.</Text>  </GameDBLocalizedString>
            
            
           <!-- After crossing the bridge two Sovereign fleet mercenaries appear. #ALFA is humanoid robot, Freddie is young, roguish pirate.--> 
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_MERCS_ALFA_1</LocID>                 <Text>My RAM is melting from assessing the threat, and you are not going to like the results, Freddie! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_MERCS_FREDDIE_1</LocID>              <Text>Shut up, #ALFA! We fuck them up! I smell an epic story, retold in space bars all over the system! We will become the shiniest chick magnets in the universe! Yeah, baby! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_MERCS_ALFA_2</LocID>                 <Text>AYAYAY, Freddie! I can't decide what I would appreciate less. The chicks or the magnets?   </Text>  </GameDBLocalizedString>
          
            
           <!-- After the firt Sovereign (merc) base is taken, Kali Chapek and Grand Manny appear to help our heroes.-->  
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_KALI_MANNY_KALI_1</LocID>            <Text>We heard you need a hand, Tin.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_KALI_MANNY_TIN_1</LocID>             <Text>Of course! But how ...?</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_KALI_MANNY_MANNY_1</LocID>           <Text>With so many Sovereigns in UNSS hands, there are lota holes in our network. We overheard the coms of those "merc" wannabes. </Text>  </GameDBLocalizedString>
           
             <!--All mercenaries are defeated the way to the Primal gate that separate the UNSS part of the map is clear.-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_MERCS_DEFEATED_RH_1</LocID>          <Text>The mercs are done! Time to press forward! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_MERCS_DEFEATED_PRINCESS_1</LocID>    <Text>If the UNSS command don't call for their fleet when I ram this gate, I don't know when!</Text>  </GameDBLocalizedString>
            
            <!--Heroes encounters Doug Porter, the friend of Annika. He was captured by UNSS in the second mission of Sovereign Fleet campaign and his faite was unknown until now.-->   
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_DOUG_1</LocID>                  <Text>... I will tell you about the Core ... #925, do you copy ...  Chiquita preheating cycle ...</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_RH_1</LocID>                    <Text>Another mercenary ... what is he mumbling?</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_TIN_1</LocID>                   <Text>Wait! It's Doug! Doug Porter! I remember him, he was a friend of Annika. Your sister caught him back on Chimera!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_PRINCESS_1</LocID>              <Text>Friend of Annika? Maybe we could persuade him to join us? </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_RH_2</LocID>                    <Text>Doug! Hey, Doug! Do you hear me?!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_DOUG_2</LocID>                  <Text>... #925, punch it, full throttle ... we gotta leave a message for my bae ... WHAT?! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_DOUG_3</LocID>                  <Text>Communication target: Captain Rao,\nHuman resource: Porter, \nReport: enemy at the gate, \nRequest: orders. </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_RAO_1</LocID>                   <Text>Poor bastard. I would not allow this treatment if I were there ... </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_BROWN_1</LocID>                 <Text>You would not move a finger. She was furious ... "Do you want to see another Solar System transport arriving through the Gate Station ... ?" </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_RAO_2</LocID>                   <Text>Sounds like Her ... Yeah, I guess I suspected this was a part of the deal when we accepted Her help back then. </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_BROWN_2</LocID>                 <Text>Rao to Porter ... engage.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_DOUG_4</LocID>                  <Text>Human resource: Porter, \nStatus: Engaging the enemy! </Text>  </GameDBLocalizedString>

            <!--Heroes encounters Doug Porter, the friend of Annika. He was captured by UNSS in the second mission of Sovereign Fleet campaign and his faite was unknown until now.-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_AF_PRINCESS_1</LocID>           <Text>Do not harm him! He's not himself.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_AF_HIERARCH_1</LocID>           <Text>Can you help him?</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_AF_PRINCESS_2</LocID>           <Text>I doubt it. I haven't encountered his psyche before. My intervention could make it worse than it already is.</Text>  </GameDBLocalizedString>
            
            <!--Princes rams the Primals gate that leads on UNSS port of the map.-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GATE_RAMMING_PRINCESS_1</LocID>      <Text>The times when I needed the gate force field weakened are only a fading memory now. </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GATE_RAMMING_BROWN_1</LocID>         <Text>The gate force field collapsed! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GATE_RAMMING_RAO_1</LocID>           <Text>Furret, what is taking you so long! We must open another front and get your Fossorians to battle now!  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GATE_RAMMING_FURRET_1</LocID>        <Text>Almost there, captain! We must recalculate the explosive volume or ... </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GATE_RAMMING_RELIC_1</LocID>         <Text>FURRET, ONE MORE EXCUSE AND I WILL EXECUTE MORE OF YOUR "BOYS" THAN COULD EVER DIE DURING THAT EXPLOSION! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GATE_RAMMING_RELIC_1.5</LocID>       <Text>RAO, IS MY FLEET IN POSITION TO START THE BOMBARDMENT? </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GATE_RAMMING_RAO_2</LocID>           <Text>The UNSS fleet is still on its way. The asteroid field in orbit is making our maneuvers difficult!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GATE_RAMMING_RELIC_2</LocID>         <Text>DO I HAVE TO MOTIVATE THEM LIKE THE FOSSORIANS?!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GATE_RAMMING_TIN_1</LocID>           <Text>The more desperate they are, the less chance they see through our plan. We must press on! Attack their bases! </Text>  </GameDBLocalizedString>

             <!--Upon conquering the Firs UNSS base the Fossorians led by Supreme leader Furret blow up the landslide. Everybody expects that they attack our heroes, but they turn the coat and join the players team.--> 
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_FURRET_PART_FURRET_1</LocID>         <Text>3, 2, 1 ... Fire in the hole! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_FURRET_PART_HOLESTER_1</LocID>       <Text>WOOOHOOO! Into the breach, boys!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_FURRET_PART_TIN_1</LocID>            <Text>Furret's Fossorians! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_FURRET_PART_VIKTAR_1</LocID>         <Text>Treacherous bastard!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_FURRET_PART_KALI_1</LocID>           <Text>I saw that coming from a mile away!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_FURRET_PART_GILEAS_1</LocID>         <Text>Regroup!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_FURRET_PART_HOLESTER_2</LocID>       <Text>That won't be necessary! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_FURRET_PART_FURRET_2</LocID>         <Text>It's time for the Fossorians to start fighting for their own future again! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_FURRET_PART_RELIC_1</LocID>          <Text>I WILL OPEN THE WAY TO YOUR HOMEWORLD, AND ONCE YOU OLD WARLORDS DEAL WITH YOU I WILL MAKE THEM MY NEW PETS! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_FURRET_PART_FURRET_3</LocID>         <Text>Will you?! Why have you kept it shut lately then? Even when the UNSS forces need restocking so desperately! You are not able to open it anymore, are you! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_FURRET_PART_RELIC_2</LocID>          <Text>FURRET, YOU FOOL ...   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_FURRET_PART_FURRET_4</LocID>         <Text>I was always a fool! Fool to risk it all! It is the Fossorian way!</Text>  </GameDBLocalizedString>
             
            <!--The second UNSS base is captured, the Relic wants to start orbital bombardment now.-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_TWO_RELIC_1</LocID>        <Text>I WANT THAT BOMBARDMENT TO START RIGHT! FUCKING! NOW! TARGET THE FOSSORIANS TOO!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_TWO_RAO_1</LocID>          <Text>There is an ongoing evacuation! Our personnel are still in the area! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_TWO_TIN_1</LocID>          <Text>What? We can spy on the UNSS communication now? </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_TWO_FURRET_1</LocID>       <Text>A little gift from old me ... I hope you have a plan, Princess. </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_TWO_GILEAS_1</LocID>       <Text>Yes, but it is not the best time for explaining ...</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_TWO_FURRET_2</LocID>       <Text>No details required. Just tell us where and when, and the hammer will fall!</Text>  </GameDBLocalizedString>

            <!--The third UNSS base is captured - pt1, UNSS fleet is ready to bombard. Princess take control over the fleet by broadcasting the litany through the ship Bright Star on the orbit.-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_RELIC_1</LocID>      <Text>GIVE THE ORDER, RAO! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_RAO_1</LocID>        <Text>NO! Our people are still down there!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_RELIC_2</LocID>      <Text>IF YOU EVER WANT TO SEE A TRANSPORT FROM THE SOLAR SYSTEM GOING THROUGH THE GATE STATION AGAIN, YOU WILL DO IT! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_BROWN_1</LocID>      <Text>I am sorry, Rao, there is no other way. Ground forces to the UNSS Fleet. Status report.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_NIOTA_1</LocID>      <Text>UNSS Rickenbacker, in position! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_TAKESHI_1</LocID>    <Text>UNSS Wrath of Khan, weapons ready! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_MAYA_1</LocID>       <Text>UNSS Normandy, missile depot unlocked! </Text>  </GameDBLocalizedString>
            
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_TIN_1</LocID>        <Text>NOW! It's time!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_PRINCESS_1</LocID>   <Text>Bright Star, do you hear me?! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_ANGAEDE_1</LocID>    <Text>YES, I AM READY. I AM READY TO BROADCAST YOUR WORD TO THE UNSS SHIPS.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_PRINCESS_2</LocID>   <Text>LET'S START ...</Text>  </GameDBLocalizedString>

            <!--The third UNSS base is captured - pt2 Princes evil sister learns what happened and teleports to orbit although it should not be possible.-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_NIOTA_2</LocID>      <Text>UNSS Rickenbacker to ground forces, reporting multiple system malfunctions!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_TAKESHI_2</LocID>    <Text>UNSS Normandy, the system is not responding; our consoles are disabled! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_MAYA_2</LocID>       <Text>Something is aiming our weapons at your position! Thomas, Rao... [SIGNAL DISTORTION]... RUN!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_BROWN_2</LocID>      <Text>Black Well of Nadir! What is happening over there?</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_RELIC_3</LocID>      <Text>THERE IS ONLY ONE EXPLANATION! THEY USED THE LITANY. THEY LEARNED THE UNSS SHIPS AI COMES FROM THE SAME SOURCE AS THE ONE OF THE CHILDREN'S SHIPS. I MUST GET TO THE ORBIT AT ONCE!  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_RELIC_4</LocID>      <Text>THERE IS SUCH A MESS IN THE PSYCHOSPHERE UP THERE! I CANNOT SEE PROPERLY ... WAIT! I SEE IT! ... A NEW PATCH!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_PRINCESS_3</LocID>   <Text>IMPOSSIBLE! How?! She just teleported to the orbit! It should not be possible!... Bright Star, do you hear me? BRIGHT STAR!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_ANGAEDE_2</LocID>    <Text>...</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_PRINCESS_4</LocID>   <Text>FUCK! I must get to Bright Star!  </Text>  </GameDBLocalizedString>

            <!--Princess realises thet there is not enough time to reach the orbit and tries to teleport as well - she appears in orbit though on wrong place. She teleports again, this time on the right spot. -->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_EMERGENCY_TELEP_PRINCESS_1</LocID>   <Text>I am not going to make it in time! I must try to teleport the same way my sister did! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_EMERGENCY_TELEP_TIN_1</LocID>        <Text>No, it's too risky! You have never tried ...  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_EMERGENCY_TELEP_PRINCESS_2</LocID>   <Text>If she takes control over the UNSS fleet again, we are dead anyway!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_EMERGENCY_TELEP_PRINCESS_3</LocID>   <Text>I nearly teleported inside that pile of junk! Ok, one more time!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_EMERGENCY_TELEP_PRINCESS_4</LocID>   <Text>I think I'm getting the hang of this! </Text>  </GameDBLocalizedString>
            
            <!--Princess is coming to ship Bright Star, in the ships shadow the Princess evil sister awaits with Annika brainwashed into serving UNSS. --> 
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_1_RELIC_1</LocID>                <Text>I KNEW THAT YOU WOULD COME, LITTLE SISTER. BUT I WILL NOT FIGHT YOU.   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_1_PRINCESS_1</LocID>             <Text>Afraid?  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_1_RELIC_2</LocID>                <Text>NO, I SIMPLY DO NOT HAVE TO. I MADE A TOOL FOR THAT SPECIAL OPPORTUNITY.    </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_1_PRINCESS_2</LocID>             <Text>Annika!?  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_1_RELIC_3</LocID>                <Text>WHAT IS LEFT OF HER.   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_1_PRINCESS_3</LocID>             <Text>What are your orders?   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_1_RELIC_4</LocID>                <Text>KILL HER! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_1_ANNIKA_1</LocID>               <Text>Yes, madam.   </Text>  </GameDBLocalizedString>
            
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_2_PRINCESS_1</LocID>             <Text>Annika! Don't listen to her! I am here with Tin! I don't want to hurt you! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_2_RELIC_1</LocID>                <Text>I KNEW YOU WOULD HOLD BACK WHEN FIGHTING THAT SCUM, SISTER! I SEE I HAVE CHOSEN THE RIGHT TOOL. </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_2_PRINCESS_2</LocID>             <Text>Fight it, Annika! You can break free!   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_2_RELIC_2</LocID>                <Text>YOU'RE WASTING YOUR BREATH! I BROKE HER DEFIANCE A LONG TIME AGO. SHE IS UNDER MY FULL CONTROL. </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_2_PRINCESS_3</LocID>             <Text>You'll pay for what you did to her! </Text>  </GameDBLocalizedString>
            
              <!-- Annika is defeated and captured by Princess.-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_3_PRINCESS_1</LocID>             <Text>Seize her! Annika must not be harmed! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_3_ANNIKA_1</LocID>               <Text>Hands off! Let me go! </Text>  </GameDBLocalizedString>
            
               <!--Princess talks to her sister and learning important information before the final fight.--> 
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_4_RELIC_1</LocID>                <Text>THAT'S UNFORTUNATE. I SHOULD HAVE REALISED THAT DEFEATING YOU IS BEYOND THE STRENGTH OF ANYONE ... EXCEPT ME! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_4_PRINCESS_1</LocID>             <Text>So what are you waiting for?   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_4_RELIC_2</LocID>                <Text>YOU WERE LOOKING FORWARD TO THIS EPIC FIGHT OF TWO SPACE GODDESSES! BUT I MUST LET YOU DOWN, SISTER. NEITHER OF US CAN USE OUR POWERS!  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_4_PRINCESS_2</LocID>             <Text>What sort of trick is this?!   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_4_RELIC_3</LocID>                <Text>NO TRICKS, IF WE FIGHT WE WILL BE OVERHEARD!   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_4_PRINCESS_3</LocID>             <Text>What are you talking about?   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_4_RELIC_4</LocID>                <Text>ABOUT OUR SISTERS FROM DIFFERENT STAR SYSTEMS! WHEN WE START, WE WON'T BE ABLE TO HOLD BACK! AND OUR KIND WILL HEAR IT!   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_4_PRINCESS_4</LocID>             <Text>That's a lie! You are trying to distract me before the fight!  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_4_RELIC_5</LocID>                <Text>WHY DO YOU THINK I STOPPED USING THE GATE STATION OR DESTROYED ALL OUR SISTERS? TO MAKE THIS SYSTEM INVISIBLE AGAIN! THEN YOU CAME AND BEGAN TO DO THE OPPOSITE!   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_4_PRINCESS_5</LocID>             <Text>I do not believe you. </Text>  </GameDBLocalizedString>
            
               <!--Princess won, but her sister decides to use her psionic powers although this will attract the unwanted attention of the other Allmothers clones in the different solar systems.-->  
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_5_PRINCESS_1</LocID>             <Text>You are defeated!  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_5_RELIC_1</LocID>                <Text>ONLY MY ARMY. BUT THE ALLMOTHER IN ME WOULD RATHER INVITE THE WHOLE UNIVERSE HERE THAN GIVE UP THE RULE TO YOU! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_5_RELIC_2</LocID>                <Text>DIE!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_5_PRINCESS_2</LocID>             <Text>So be it!</Text>  </GameDBLocalizedString>
            
               <!-- It's over, Tin comes to the Princess acknowledging her victory. She shared information about the possible future event. End cutscene.-->  
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_TIN_1</LocID>                    <Text>You did it! You won! We ... WHAT?! Is this Annika? ... Do you hear me, Starling?  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_PRINCESS_1</LocID>               <Text>YES, she's supposed to be my sister's last attempt to defeat me without using her powers. She wanted to hide this system from the rest my kind.   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_TIN_2</LocID>                    <Text>But you used your powers! I have seen the blast! I ... I do not understand!   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_PRINCESS_2</LocID>               <Text>I will explain. But I am worried, my victory will bring us only a short moment of peace. If my kind comes, we have to be prepared. We will need this system unified.   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_TIN_3</LocID>                    <Text>You're thinking of accepting your legacy! Embracing the Allmother in you?   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_PRINCESS_3</LocID>               <Text>Not now, but I fear that soon there will be no other choice.</Text>  </GameDBLocalizedString>
            
      <!--      BEFORE/AFTER FIGHT QUOTES-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_MERCS_FREDDIE_2</LocID>              <Text>You broke Alfa! Most of his parts were custom-made! ... I'll pay for the repairs from the bonus I get for your head! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_MERCS_ALFA_3</LocID>                 <Text>AYAYAY! Freddie wait! I am going to your aid, my friend!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_MERCS_RH_1</LocID>                   <Text>...another one bites the dust.</Text>  </GameDBLocalizedString>
            
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GAHIJI_AF_GAHIJI_1</LocID>           <Text>We are getting overrun! Aim the artillery at my position!  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GAHIJI_AF_BROWN_1</LocID>            <Text>As you wish. On three ...   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GAHIJI_AF_RAO_1</LocID>              <Text>No!  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GAHIJI_AF_BROWN_2</LocID>            <Text>Al right, captain No-fun.  </Text>  </GameDBLocalizedString>
            
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_ADELA_BF_RAO_1</LocID>               <Text>Get your men out of there, Adela!  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_ADELA_BF_ADELA_1</LocID>             <Text>Imposible, we are surrounded, captain ... I ... [RADIO STATIC]   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_ADELA_BF_RAO_2</LocID>               <Text>Adela ... Adela! ... Got damn it!  </Text>  </GameDBLocalizedString>

