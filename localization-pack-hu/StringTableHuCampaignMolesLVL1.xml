        <LocalizedStrings>
            <!--DIALOGUES-->

            <!--NOTE ABOUT CHARACTERS 
            KRTKOV in different text IDs is Furret
            DIGIN in different text IDs is Muff
            -->
            
            <!-- Snif and Furret - opening dialog-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_START_SNIF1</LocID>                      <Text><![CDATA[Büntető légió, ez a mélypont, Furret.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_START_KRTKOV1</LocID>                    <Text><![CDATA[Nem sokáig, Sniff. A Legfelsőbb Vezető megígérte, hogy felszabadít minden önkéntest!]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_START_SNIF2</LocID>                      <Text><![CDATA[De addig kevesebb vagy, mint egy csörgő Patkány csöppség. \És úgy fogsz megszólítani, hogy "uram"!]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_START_SNIF6</LocID>                      <Text><![CDATA[Most jól figyelj! Egy tekintélyes csoport ellenséges Kiberbogár ás a romokban délkeleten. Aktiváld ott a rádiójeladót, és kisütjük őket a pályáról. ]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_START_KRTKOV3</LocID>                    <Text><![CDATA[De a kísérőm túl kicsi egy ilyen feladathoz! ]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_START_SNIF4</LocID>                      <Text><![CDATA[Az élcsapat bázist hozott létre délen, és friss csapatok várnak rátok néhány kiválasztott gyülekezőponton.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_START_SNIF5</LocID>                      <Text><![CDATA[Most pedig menj! És ne feledd, ez egy titkos művelet! Nem kockáztathatunk meg egy teljes körű háborút a Kiberbogarak ellen... Mégis.]]></Text>  </GameDBLocalizedString>
            
            <!-- Furret is angry at Snif -->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_SNIF_COMMNETARY_KRTKOV1</LocID>          <Text><![CDATA[Emlékszem arra az arrogáns idiótára és egy napra; Tízszeresen fizetek neki.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_SNIF_COMMNETARY_SNIF</LocID>             <Text><![CDATA[Ezt kétlem. Most halkítsd le a rövidhullámot, te bolond!]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_SNIF_COMMNETARY_KRTKOV2</LocID>          <Text><![CDATA[Ó nem! nem úgy értettem...! Átkozott!]]></Text>  </GameDBLocalizedString>

            <!--Spiders-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_SPIDERS_KRTKOV</LocID>                   <Text><![CDATA[Pókok?! Utálom a pókokat! Öld meg őket! Öld meg őket, mielőtt tojást raknak!! ]]></Text>  </GameDBLocalizedString>

            <!--SNAILS-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_SLURM_FURRET</LocID>                     <Text><![CDATA[Hozzatok egy kis sót, fiúk!]]></Text>  </GameDBLocalizedString>
																															<!-- OR if you meant "Get some of my salt" then, "Get some of me salt, boys!" -->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_SLURM_MUFF</LocID>                       <Text><![CDATA[Zuhanj előre! Vigyázat, csapatok! Savat hánynak, ami átolvad a fémen!]]></Text>  </GameDBLocalizedString>


            <!--Primal Collector-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_COLECTOR_KRTKOV</LocID>                  <Text><![CDATA[Ó! Ez a robot évszázadosnak tűnik, de lefogadom, hogy még mindig ellenséges. Talán egy régi Ősiek általi alkotás. ]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_COLECTOR_MUFF</LocID>                    <Text><![CDATA[Az Ősi Emberek által létrehozott Gyűjtők! Mestereik már rég elmentek, de semmi sem változott számukra. ]]></Text>  </GameDBLocalizedString>


            <!--Base discovery - two event triggets, one 3x3x on the road and one on the whatchtower, the setup ensures this will only trigger once-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_BASE_DISCOVERY_KRTKOV1</LocID>           <Text><![CDATA[A bázis ablakai még mindig világítanak. Ez jó jel!]]></Text>  </GameDBLocalizedString>

            <!--Rattling deserters seen from a distance  -->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_SECOND_ENEMY_KRTKOV</LocID>              <Text><![CDATA[Bányászpatkányok! Azok a kis félvérek a mi oldalunkon állnak – de miért kóborolnak parancsnok nélkül? Móresre kéne tanítanom őket!]]></Text>  </GameDBLocalizedString>

            <!-- Rattlings - dialog -->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_FIRST_ENEMY_MEETING_KRTKOV2</LocID>      <Text><![CDATA[Merre jártok félvérek?! A csata a másik út!]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_FIRST_ENEMY_MEETING_RATLINGS2</LocID>    <Text><![CDATA[Mi... ööö... újra csoportosítunk... ööö... parancsolunk... ]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_FIRST_ENEMY_MEETING_KRTKOV3</LocID>      <Text><![CDATA[Értem. Még szerencsétek, hogy nem egy Komisszár kísér engem!!]]></Text>  </GameDBLocalizedString>

            <!-- Rattling diggers - dialog -->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_RATLINGS_DIGGERS_KRTKOV1</LocID>         <Text><![CDATA[Mit csináltatok, mocskos félvérek?! Nem mehetünk át!]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_RATLINGS_DIGGERS_RATLINGS1</LocID>       <Text><![CDATA[Ásunk. Megrendelések. Kevesen vagyunk. Sok ásás lesz!! ]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_RATLINGS_DIGGERS_KRTKOV2</LocID>         <Text><![CDATA[Értem. Még szerencse, hogy addig is dolgom van. De ássatok gyorsabban, idióták! ]]></Text>  </GameDBLocalizedString>

            <!-- meatboys deserter -->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_MEATBOYS_DEFECTORS_KRTKOV1</LocID>       <Text><![CDATA[Ez a teljes káosz! Hol van a parancsnokod?]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_MEATBOYS_DEFECTORS_DIGIN1</LocID>        <Text><![CDATA[Csak a legnagyobb harcosoknak engedelmeskedünk. Nem vagy nagy harcos. Te csak egy kövér vagy!]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_MEATBOYS_DEFECTORS_KRTKOV2</LocID>       <Text><![CDATA[De tábornok vagyok! Fizetni fog ezért a sértésért!]]></Text>  </GameDBLocalizedString>

            <!--watchtower -->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_WATCHTOWER1_KRTKOV</LocID>               <Text><![CDATA[Egy Figyelőtorony!]]></Text>  </GameDBLocalizedString>

            <!--DIALOG - Furret and Muff, first meeting-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_MEETING_KRTKOV1</LocID>                  <Text><![CDATA[Dicsőség a Legfelsőbb Vezetőnek!]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_MEETING_DIGIN1</LocID>                   <Text><![CDATA[Kétlem, hogy a Büntető Légió zászlója dicsőséget hozna neki vagy nekünk.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_MEETING_KRTKOV2</LocID>                  <Text><![CDATA[Úgy teszek, mintha nem hallottam volna. \n\nFurret tábornok vagyok, és önnek az élcsapat tagjának kell lennie.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_MEETING_DIGIN2</LocID>                   <Text><![CDATA[Muff a neven.\n\nEgykor pilóta voltam, most a munkaügyi egység Parancsnoka vagyok, akinek a feladata, e Bázis építésének befejezése.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_MEETING_KRTKOV3</LocID>                  <Text><![CDATA[Tehát technikailag én vagyok a felettese!]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_MEETING_DIGIN3</LocID>                   <Text><![CDATA[Feltételezem, hogy igen, de a Bázis befejezésének parancsa feljebb való főbb parancstól származik, tehát felülír téged. ]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_MEETING_KRTKOV4</LocID>                  <Text><![CDATA[Nos, ez a szerencsés napod! Egy sereget kell toboroznom ezen a Bázison. Szóval, segítek.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_MEETING_DIGIN4</LocID>                   <Text><![CDATA[Valószínűleg megbánom, de egyelőre nincs vesztenivalóm.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_MEETING_DIGIN5</LocID>                   <Text><![CDATA[Akkor térjünk egyenesen a lényegre. A forráshiány hátráltatta az építkezést. Elküldtem a Földrengetőinket egy közeli Limerit Kőbányába, de még nem tértek vissza.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_MEETING_KRTKOV6</LocID>                  <Text><![CDATA[A Bázis vizsgálatának befejezése után azonnal ellenőrizni fogom a Kőbányát!]]></Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_MEETING_DIGIN7</LocID>                   <Text><![CDATA[Hadd mutassam meg az utat.]]></Text>  </GameDBLocalizedString>

            <!--hak the rat dialogue-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_HAK_KRTKOV1</LocID>                      <Text><![CDATA[[Ah, erősítésiem! Dicsőség a Legfelsőbb Vezetőnek!]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_HAK_HAK1</LocID>                         <Text><![CDATA[Ó, fogd be! Itt nem hall téged, te csizmadia!\nCsak mi ketten vagyunk, és csak egy vezetheti a sereget. Hajlítsa be a térdét! ]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_HAK_KRTKOV2</LocID>                      <Text><![CDATA[Nem! Az utasításaim egyértelműen...]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_HAK_HAK2</LocID>                         <Text><![CDATA[Utasítás? A parancs nem érdekel! Csak az eredmények számítanak, és csak én tudom azokat szállítani!]]></Text>  </GameDBLocalizedString>

            <!--After Hak the Rat dialogue with Snif-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_REINF3_KRTKOV1</LocID>                   <Text><![CDATA[Lázadás! A Legfelsőbb Vezetőt tájékoztatni kell. ]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_REINF3_SNIF1</LocID>                     <Text><![CDATA[Mit akar, Furret?! Kössem meg a cipőfűzőjét? ]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_REINF3_KRTKOV2</LocID>                   <Text><![CDATA[A gyülekezőhelyen lévő csapatok fellázadtak! El kellett őket pusztítanom! De még mindig erősítésre van szükségem! ]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_REINF3_SNIF2</LocID>                     <Text><![CDATA[Több csapatra van szüksége? Akkor HAGYJA MEG A MEGÖLÉSÜKET! Nem érdekel, hogyan, de az a jeladó ELÉR A KOORDINÁTÁKHOZ! ]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_REINF3_KRTKOV3</LocID>                   <Text><![CDATA[A Legfelsőbb Vezető soha nem tenné...]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_REINF3_DIGIN3</LocID>                    <Text><![CDATA[Igen, elmondhatja neki, ha befejezte a küldetést. Most pedig MOZGASSA MEG A LUSTA SEGGÉT! Több Büntető Légiós Parancsnok várja, hogy átvegye a helyét. TISZTÁN ÉRTHETŐ VOLTAM?]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_REINF3_KRTKOV4</LocID>                   <Text><![CDATA[Kristály tisztán.]]></Text>  </GameDBLocalizedString>

            <!--Sniff sends the first commander to replace Furret -->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_FOSS_HERO1_SNIF1</LocID>                 <Text><![CDATA[Hé! Furret, mit csinálsz odalent? Az orbitális lézer közeledik az optimális pozícióhoz! Jelentés!]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_FOSS_HERO1_KRTKOV2</LocID>               <Text><![CDATA[Biztosítottuk a Bázist, és stabilan dolgozunk... ]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_FOSS_HERO1_SNIF2</LocID>                 <Text><![CDATA[Mostanra be kellett volna jutnod a romok közé, te alkalmatlan!... Elég ebből... Küldök egy cserét!]]></Text>  </GameDBLocalizedString>

            <!--Sniff sends the second commander to replace Furret -->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_FOSS_HERO2_SNIF1</LocID>                 <Text><![CDATA[Furret! Fogy az idő! A Jeladónak már jelet kellene küldenie, de még a romokhoz sem ért!]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_FOSS_HERO2_KRTKOV2</LocID>               <Text><![CDATA[Minden tiszteletem mellett, uram, ön nincs itt! ]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_FOSS_HERO2_SNIF2</LocID>                 <Text><![CDATA[Ez kifogás?! Elég! Küldök valakit, aki képes előrelépni! ]]></Text>  </GameDBLocalizedString>

            <!--Buletproof benni - before fight-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_FOSS_HERO1_BF_BENI</LocID>               <Text><![CDATA[Micsoda megtiszteltetés! A főparancsnokság úgy gondolja, hogy a fejlődésed megtorpant, ezért engem küldtek. ENGEM! Most légy jó fickó, és békésen add át a Parancsnokságot. ]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_FOSS_HERO1_BF_KRTKOV2</LocID>            <Text><![CDATA[Ó, nézd! FÖLDTÚRÓ RUHÁBAN EGY PATKÁNY! Csörög, nyikorog, nyikorog... Csak valami állati zajt hallok, te hülye kártevő! Most megmutatom a békés behódolást!]]></Text>  </GameDBLocalizedString>
            
            <!-- JB: wiki: wager of battle, trial by battle? -->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_FOSS_HERO1_BF_DIGIN2</LocID>             <Text><![CDATA[Az ősi törvények szerint csatapróba dönti el a vezetést!]]></Text>  </GameDBLocalizedString>

            <!--Buletproof benni - after fight-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_FOSS_HERO1_AF_FURRET</LocID>             <Text><![CDATA[ADD ODA A RÁDIÓT! Egy agyhalott félvért küldtél helyettem?! MILYEN SÉRTÉS! ]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_FOSS_HERO1_AF_SNIF</LocID>               <Text><![CDATA[Ez elég ígéretesnek tűnt. Nos, megvédted a parancsodat... Egyelőre. ]]></Text>  </GameDBLocalizedString>

            <!--Moleson - before fight-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_FOSS_HERO2_BF_MOLESON</LocID>            <Text><![CDATA[Ne gondolja, hogy örülök, hogy engem választottak csereként, de a dolgok sokkal simábban mennek majd, ha én leszek a felelős. ]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_FOSS_HERO2_BF_KRTKOV</LocID>             <Text><![CDATA[Fogadok, hogy önként jelentkezett.]]></Text>  </GameDBLocalizedString>

            <!--Moleson - after fight-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_FOSS_HERO2_AF_MOLESONT</LocID>           <Text><![CDATA[Nagyon hatékonyan véded a parancsodat. Nem tudom felfogni, miért nem tudod elszállítani folyamatosan azt a jeladót!  ]]></Text>  </GameDBLocalizedString>

            <!--  Furret considers his chances-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_BASE_FURRET</LocID>                      <Text><![CDATA[Ennyit az erősítésekről...]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_BASE_FURRET1</LocID>                     <Text><![CDATA[A Bázis, amit az őrtoronyból láttam, most az egyetlen esélyem!]]></Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_REINF_FAIL_FURRET</LocID>                <Text><![CDATA[Senki sem marad itt, hogy megerősítse csapataimat, de van még egy gyülekezőpont!]]></Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_RETREAT_NOOPTION</LocID>                <Text><![CDATA[Semmi esetre sem megyek arrafelé. csak lelőnének.]]></Text>  </GameDBLocalizedString>


            <!-- Furret encounters Knights in the north-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_KNIGHT_DIVERSION_Furret1</LocID>         <Text><![CDATA[Mit tettél? Hol vannak azok a Földtúrók, akiknek itt kellett volna várniuk? ]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_KNIGHT_DIVERSION_KNIGHT1</LocID>         <Text><![CDATA[A kártevőt megölték. Ledöntöttük az oszlopot, és az utat elzárták. A hangunk elnémulhat, de te soha nem léphetsz be a romok közé. Elfogadható áldozat.]]></Text>  </GameDBLocalizedString>
            
            <!-- Furrets encounter Knights by the north earthshaker - narrator-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_DIVERSION_NARRATOR</LocID>               <Text><![CDATA[Most hallottam egy robbanást dél felől. Ott várnia kell rám az erősítésnek! Sietnem kell!]]></Text>  </GameDBLocalizedString>

            <!--Pickups site quest-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_FIRST_PICKUP_DIGIN1</LocID>              <Text><![CDATA[Biztosan nem ez az utolsó hasznos játék!? Jó lenne jól felszerelt Kiberbogarakkal szembenézni!]]></Text>  </GameDBLocalizedString>
         
            <!-- Limerite mine-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_LIM_QUARRY_PART4_KRTKOV1</LocID>         <Text><![CDATA[Ennyi! Limerit! Tudtad, hogy a Legfelsőbb Vezető találta ki, hogyan juthatnak mikrobák a benne lévő apró légbuborékokba? És azok a kis ördögök kijavítják a repedéseket és egyebeket!]]></Text> </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_LIM_QUARRY_PART4_DIGIN1</LocID>          <Text><![CDATA[Nehéz elhinni. Ezt tanultad a katonai akadémián?]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_LIM_QUARRY_PART4_KRTKOV2</LocID>         <Text><![CDATA[Nem. Ha tudnia kell, a forradalom idején csatlakoztam. Azelőtt én... Nem számít. Csak a köpönyegforgatóknak van tudományos múltja.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_MOLES_LVL1_LIM_QUARRY_PART4_DIGIN2</LocID>          <Text><![CDATA[...igen, köpönyegforgatók. Megbízhatatlanok. ]]></Text>  </GameDBLocalizedString>

            <!-- BIOFARM -->
