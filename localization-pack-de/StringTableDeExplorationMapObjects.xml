            <GameDBLocalizedString>         <LocID>MAPOBJ_GOLD_MINE</LocID>                                           <Text>Verhüttung</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MAPOBJ_GOLD_MINE_DESCRIPTION</LocID>                               <Text><![CDATA[Produziert <color=SOTS_COLOR_GOLD>500</color> (<color=SOTS_COLOR_GOLD>1.000</color>, wenn aufgewertet) <color=SOTS_COLOR_GOLD>Aura-Münzen</color> pro Zug.]]></Text>  </GameDBLocalizedString>
                                                                                                                      
            <GameDBLocalizedString>         <LocID>MAPOBJ_WATCHTOWER_01</LocID>                                       <Text>Wachturm</Text>  </GameDBLocalizedString>
