        <LocalizedStrings>
            <!--OBJECTIVES MAIN QUESTS-->
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_START_LVL4</LocID>                      <Text>Atraia a Frota NUSS</Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_CRUS_BASES_1</LocID>               <Text>Ocupe as bases próximas 0/4</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_CRUS_BASES_2</LocID>               <Text>Ocupe as bases próximas 1/4</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_CRUS_BASES_3</LocID>               <Text>Ocupe as bases próximas 2/4</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_CRUS_BASES_4</LocID>               <Text>Ocupe as bases próximas 3/4</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_CRUS_BASES_DONE</LocID>            <Text><![CDATA[<color=SOTS_COLOR_MOVEMENT>Ocupe as bases próximas 4/4 - CONCLUIDO</color>]]></Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_STASH_1</LocID>                    <Text>Pegue os recursos 0/3</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_STASH_2</LocID>                    <Text>Pegue os recursos 1/3</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_STASH_3</LocID>                    <Text>Pegue os recursos 2/3</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_STASH_DONE</LocID>                 <Text><![CDATA[<color=SOTS_COLOR_MOVEMENT>Pegue os recursos 3/3 - CONCLUIDO</color>]]></Text>  </GameDBLocalizedString>


            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_CROSS_BRIDGE</LocID>               <Text>Cruze a ponte</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_CROSS_BRIDGE_DONE</LocID>          <Text><![CDATA[<color=SOTS_COLOR_MOVEMENT>Cruze a ponte - CONCLUIDO</color>]]></Text>  </GameDBLocalizedString>
            
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_REPAIR_TELEPORT</LocID>            <Text><![CDATA[Conserte o transportador com a <color=SOTS_COLOR_RED>Princesa</color>]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_REPAIR_TELEPORT_DONE</LocID>       <Text><![CDATA[<color=SOTS_COLOR_MOVEMENT>Conserte o transportador com a Princesa - CONCLUIDO</color>]]></Text>  </GameDBLocalizedString>
            
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_OPEN_BRIDGE</LocID>                <Text><![CDATA[Abra a ponte com a <color=SOTS_COLOR_RED>Princesa</color>]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_OPEN_BRIDGE_DONE</LocID>           <Text><![CDATA[<color=SOTS_COLOR_MOVEMENT>Abra a ponte com a Princesa - CONCLUIDO</color>]]></Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_SF_BASES_1</LocID>                 <Text>Tome as bases mercenárias 0/2</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_SF_BASES_2</LocID>                 <Text>Tome as bases mercenárias 1/2</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_SF_BASES_DONE</LocID>              <Text><![CDATA[<color=SOTS_COLOR_MOVEMENT>Tome as bases mercenárias 2/2 - CONCLUIDO</color>]]></Text>  </GameDBLocalizedString>
           
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_SF_MERCHS_1</LocID>                <Text>Derrote os mercenários 0/2</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_SF_MERCHS_2</LocID>                <Text>Derrote os mercenários 1/2</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_SF_MERCHS_DONE</LocID>             <Text><![CDATA[<color=SOTS_COLOR_MOVEMENT>Derrote os mercenários 2/2 - CONCLUIDO</color>]]></Text>  </GameDBLocalizedString>
  
          
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_RAM_GATE</LocID>                   <Text><![CDATA[Derrube o portão com a <color=SOTS_COLOR_RED>Princesa</color>]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_RAM_GATE_DONE</LocID>              <Text><![CDATA[<color=SOTS_COLOR_MOVEMENT>Derrube o portão com a Princesa - CONCLUIDO</color>]]></Text>  </GameDBLocalizedString>
            
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASES_1</LocID>               <Text>Capture as bases NUSS 0/4</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASES_2</LocID>               <Text>Capture as bases NUSS 1/4</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASES_3</LocID>               <Text>Capture as bases NUSS 2/4</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASES_4</LocID>               <Text>Capture as bases NUSS 3/4</Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_GET_TO_SHIP</LocID>                <Text><![CDATA[<color=SOTS_COLOR_RED>Leve a Princesa a nave Estrelha Brilhante - turno(s) restante(s):</color>]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>OBJECTIVE_CAMPAIGN_CRUSADERS_LVL4_GET_TO_SHIP_DONE</LocID>           <Text><![CDATA[<color=SOTS_COLOR_MOVEMENT>Leve a Princesa a nave Estrelha Brilhante - CONCLUIDO</color>]]></Text>  </GameDBLocalizedString>
            
            <!--DIALOGUES-->
            <!--Heroes arrived to the orbit of planet Sirena, on the surface there is main base Princess evil sister. They are standing next to ancient battleship Bright Star.   -->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_NARRATOR_1</LocID>             <Text>No meio do campo de asteróides de Siren, o quarto planeta do Sistema... </Text>  </GameDBLocalizedString>
            
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_VIKTAR_1</LocID>               <Text>Que engraçado! Agora, será que Vossa Alteza poderia ter a gentileza de nos contar o verdadeiro plano? </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_PRINCESS_1</LocID>             <Text>Não era piada, Viki.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_VIKTAR_2</LocID>               <Text>Então desejava que realmente fosse! Assim que a frota NUSS chegar aqui, seremos obliterados da órbita. É simples assim! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_PRINCESS_2</LocID>             <Text>Não vai acontecer. Assim que as naves da NUSS chegarem, chamarei a Estrela Brilhante...</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_ANGAEDE_1</LocID>              <Text>... E USAREI A MINHA CONEXÃO COM AS NAVES NUSS PARA TRANSMITIR A VOZ DA PRINCESA, PORTANDO AS PALAVRAS DA LITANIA.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_TIN_1</LocID>                  <Text>Com a frota NUSS nas nossas mãos, dá pra fazê-los provarem do próprio remédio! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_PRINCESS_2.5</LocID>           <Text>Tenho certeza que até a minha irmã não consegue sobreviver a um bombardeio orbital. </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_GILEAS_1</LocID>               <Text>Mas por que agir tão cedo? As palavras da Estrela Brilhante condenam o Grande Zieregal dos crimes no qual ele culpava a Princesa e os outros clones da Mãe-de-Todos. Assim que os Filhos souberem da verdade, virão ao nosso socorro assim como eu. </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_PRINCESS_3</LocID>             <Text>Assim que a minha irmã souber da Estrela Brilhante, vai tentar roubá-la da mesma forma que queremos roubar a frota NUSS. Temos que atacar agora. </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_VIKTAR_3</LocID>               <Text>Então por que Vossa Alteza nos fez desembarcar na órbita?   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_TIN_2</LocID>                  <Text>Durante a Guerra Santa, até a órbita era um campo de batalha, e depois de todo esse tempo, as bases ainda estão aqui, então ainda temos tempo de nos prepararmos. \n\nAssim que chegarmos a superfície, seremos descobertos.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_VIKTAR_4</LocID>               <Text>Certo, eu desisto... por hora. </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_PRINCESS_4</LocID>             <Text>Vamos nessa!</Text>  </GameDBLocalizedString>
          
            <!--After the dialog, everybody is teleported to their bases, Princess turns around and deactivates the bridge leading to Bright Star orbital islet.--> 
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_START_PRINCESS_5</LocID>             <Text>Por via das dúvidas.</Text>  </GameDBLocalizedString> 
           
            <!--All Childrens bases are captured, now the heroes focus on gathering resources and exploration. Some production building are missing in orbit though. -->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_CRUS_BASES_PRINCESS_1</LocID>        <Text>Ótimo, são todas elas.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_CRUS_BASES_TIN_1</LocID>             <Text>Alguns recursos não podem ser produzidos aqui em cima. Acho que teremos que ir a superfície mais cedo que o esperado.  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_CRUS_BASES_PRINCESS_2</LocID>        <Text>Irei ativar o transportador, mas não até rasparmos o fundo do pote. Há alguns naufragios por perto... </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_CRUS_BASES_GILEAS_1</LocID>          <Text>Estamos roubando túmulos, agora?  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_CRUS_BASES_VIKTAR_1</LocID>          <Text>Tire o cabo do seu reto, humano. Os mortos não precisam de carga. </Text>  </GameDBLocalizedString>

             <!--Time to repair the teleport down.-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_REPAIR_TELEPORT_RH_1</LocID>         <Text>Todos os maiores cachês de recursos foram coletados. Hora de abrir o transportador.  </Text>  </GameDBLocalizedString>
        
            <!-- Teleport is repaired.-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_TELEPORT_REPAIRED_PRINCESS_1</LocID> <Text>O caminho abaixo está pronto.</Text>  </GameDBLocalizedString>
            
            <!--  Princes, has used the teleport and descended to the ground.-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_ON_THE_GROUND_PRINCESS_1</LocID>     <Text>Estou na superfície.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_ON_THE_GROUND_GILEAS_1</LocID>       <Text>Consegue usar essa sua "Psico-esfera" pra checar a frente?  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_ON_THE_GROUND_PRINCESS_2</LocID>     <Text>Não, isso vai alertar a minha irmã.  </Text>  </GameDBLocalizedString>

            <!--Part of the map is separated by the landslide, but the road suggest the area was passable once.-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_LAND_SLIDE_RH_1</LocID>              <Text>A estrada foi bloqueada por um deslizamento. Uma explosão limparia o caminho, mas seria o mesmo que soar uma tromba de guerra.  </Text>  </GameDBLocalizedString>

            <!--Hero encounters first UNSS commander. The UNSS commander runs to the near bridge and cross to avoid confrontation.-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GAHIJI_REPORT_GAHIJI_1</LocID>       <Text>Capitão Rao! Temos companhia!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GAHIJI_REPORT_RAO_1</LocID>          <Text>Abram a ponte e mandem mercenários para defendê-la! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GAHIJI_REPORT_GAHIJI_2</LocID>       <Text>Sim, senhor!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GAHIJI_REPORT_RH_1</LocID>           <Text>E agora começa.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GAHIJI_REPORT_PRINCESS_1</LocID>     <Text>No lado positivo, não faz mais sentido deixar a Psico-esfera de lado. </Text>  </GameDBLocalizedString>
            
           <!-- Hero get close to the now deactivated bridge and overhears the conversation of enemy commanders. Gahiji reports he spotted enemy to Captain Rao and Dr. Brown 9B. -->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_BRIDGE_DIALOG_GAHIJI_1</LocID>       <Text>... não muito longe daqui.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_BRIDGE_DIALOG_RAO_1</LocID>          <Text>Percebi... bom trabalho em desligar a ponte, Comandante Gahiji. Não dá pra subestimá-los desta vez.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_BRIDGE_DIALOG_BROWN_1</LocID>        <Text>Quanta tensão na sua voz, Rao! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_BRIDGE_DIALOG_RAO_2</LocID>          <Text>Com todo e bom motivo, 9B! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_BRIDGE_DIALOG_BROWN_2</LocID>        <Text>Gergie-larh, quer saber uma coisa, seu piratinha?! ...você tem razão, eu vacilei. Agora, quem irá avisar ela?  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_BRIDGE_DIALOG_RAO_3</LocID>          <Text>Eu faço isso. Ela arrancaria a sua cabeça se trouxesse mais más noticias pra ela. </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_BRIDGE_DIALOG_BROWN_3</LocID>        <Text>Acho que Ela faria isso, sim... Obrigado, Rao. </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_BRIDGE_DIALOG_RAO_4</LocID>          <Text>Tem mais uma coisa a ser feita. Vamos chamar o Furret.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_BRIDGE_DIALOG_RAO_5</LocID>          <Text>Líder Supremo, algum progresso quanto aquelas pedras caídas bloqueando a Via Leste?   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_BRIDGE_DIALOG_FURRET_1</LocID>       <Text>Essas coisas não podem ser feitas com pressa, Capitão. Um simples erro de cálculo aqui, e o colapso pode ser maior ainda! Tenha paciência.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_BRIDGE_DIALOG_TIN_1</LocID>          <Text>Furret está aqui também?! Aquele canalha traiçoeiro está tentando nos aproximar por trás!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_BRIDGE_DIALOG_PRINCESS_1</LocID>     <Text>Vamos ter que nos preparar pra essa alternativa, também.</Text>  </GameDBLocalizedString>
            
            <!-- Princess uses her abilities to activate the bridge. -->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_ACTIVATE_BRIDGE_PRINCESS_1</LocID>   <Text>Pontes desativadas, minha especialidade.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_ACTIVATE_BRIDGE_PRINCESS_2</LocID>   <Text>Molezinha.</Text>  </GameDBLocalizedString>
            
            
           <!-- After crossing the bridge two Sovereign fleet mercenaries appear. #ALFA is humanoid robot, Freddie is young, roguish pirate.--> 
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_MERCS_ALFA_1</LocID>                 <Text>Minha RAM está fritando só de calcular as ameaças, e você não vai gostar dos resultados, Freddie! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_MERCS_FREDDIE_1</LocID>              <Text>Cala essa boca, #ALFA! A gente vai é foder com eles! É cheiro de história épica, contada em bares espaciais por todo o sistema! Vamos virar os maiores pegadores de gatinhas do universo inteiro! Isso aí, nenem! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_MERCS_ALFA_2</LocID>                 <Text>Ai ai ai, Freddie! Agora não sei o que eu gosto menos: As gatinhas ou os bares.   </Text>  </GameDBLocalizedString>
          
            
           <!-- After the firt Sovereign (merc) base is taken, Kali Chapek and Grand Manny appear to help our heroes.-->  
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_KALI_MANNY_KALI_1</LocID>            <Text>Soubemos que precisava de uma mãozinha, Lata.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_KALI_MANNY_TIN_1</LocID>             <Text>Claro que sim! Mas como...?</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_KALI_MANNY_MANNY_1</LocID>           <Text>Com tantos Soberanos nas mãos da NUSS, tem muitos buracos na nossa rede. Escutamos a comunicação desses metidos a "mercenários". </Text>  </GameDBLocalizedString>
           
             <!--All mercenaries are defeated the way to the Primal gate that separate the UNSS part of the map is clear.-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_MERCS_DEFEATED_RH_1</LocID>          <Text>Os mercenários já eram! Hora de seguir em frente! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_MERCS_DEFEATED_PRINCESS_1</LocID>    <Text>Se o comando da NUSS não chamar a frota assim que eu derrubar esse portão, não sei mais quando chamam!</Text>  </GameDBLocalizedString>
            
            <!--Heroes encounters Doug Porter, the friend of Annika. He was captured by UNSS in the second mission of Sovereign Fleet campaign and his faite was unknown until now.-->   
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_DOUG_1</LocID>                  <Text>...vou lhe falar sobre o Núcleo... #925, está me ouvindo...  ciclo de pré-aquecimento da Chiquita...</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_RH_1</LocID>                    <Text>Outro mercenário... por que ele está balbuciando?</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_TIN_1</LocID>                   <Text>Espera! É o Doug! Doug Porter! Me lembro dele, era um amigo da Annika. Sua irmã o pegou antes, em Chimera!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_PRINCESS_1</LocID>              <Text>Amigo da Annika? Talvez dê pra persuadí-lo a se juntar a nós? </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_RH_2</LocID>                    <Text>Doug! Ei, Doug! Tá me ouvindo?</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_DOUG_2</LocID>                  <Text>... #925, manda ver, força total ... temos que dar uma mensagem a minha linda ... QUÊ?! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_DOUG_3</LocID>                  <Text>Alvo de contato: Capitão Rao. \nRecurso Humano: Porter. \nRelatório: Inimigo no portão. \nSolicitação: órdens. </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_RAO_1</LocID>                   <Text>Pobre desgraçado. Não teria permitido isso, se eu estivesse lá... </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_BROWN_1</LocID>                 <Text>Você não mexeria um dedo. Ela estava furiosa... "Você quer ver outro transporte do Sistema Solar chegando pela Estação do Portal!?" </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_RAO_2</LocID>                   <Text>Parece que nem Ela... \n\nÉ, suspeitei que isso era parte do acordo quando aceitamos a ajuda dela antes. </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_BROWN_2</LocID>                 <Text>Rao para Porter. Ataque.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_DOUG_4</LocID>                  <Text>Recurso Humano: Porter. \nStatus: Atacando o inimigo. </Text>  </GameDBLocalizedString>

            <!--Heroes encounters Doug Porter, the friend of Annika. He was captured by UNSS in the second mission of Sovereign Fleet campaign and his faite was unknown until now.-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_AF_PRINCESS_1</LocID>           <Text>Não machuquem ele! Foi lavagem cerebral.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_AF_HIERARCH_1</LocID>           <Text>Consegue ajudá-lo?</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_DOUG_AF_PRINCESS_2</LocID>           <Text>Duvido. Nunca encontrei o subconsciente dele, antes. Qualquer intervenção minha pode piorar tudo.</Text>  </GameDBLocalizedString>
            
            <!--Princes rams the Primals gate that leads on UNSS port of the map.-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GATE_RAMMING_PRINCESS_1</LocID>      <Text>As épocas de quando eu precisava dos campos de força dos Portões fracos estão virando lembrança, agora... </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GATE_RAMMING_BROWN_1</LocID>         <Text>O campo de força do Portão caiu! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GATE_RAMMING_RAO_1</LocID>           <Text>Furret, que diabos tá demorando tanto!? Temos que abrir outro fronte e levar seus Fossorianos a batalha! AGORA!  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GATE_RAMMING_FURRET_1</LocID>        <Text>Quase lá, Capitão! Temos que recalcular o volume de explosivos ou- </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GATE_RAMMING_RELIC_1</LocID>         <Text>FURRET, MAIS UMA DESCULPA E EU IREI EXECUTAR MAIS DE SEUS "RAPAZES" DO QUE ELES PODERIAM MORRER NAQUELA EXPLOSÃO! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GATE_RAMMING_RELIC_1.5</LocID>       <Text>RAO, A MINHA FROTA ESTÁ EM POSIÇÃO PRA INICIAR O BOMBARDEIO? </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GATE_RAMMING_RAO_2</LocID>           <Text>A frota NUSS ainda está a caminho. Esse campo de asteróides na órbita está complicando nossas manobras!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GATE_RAMMING_RELIC_2</LocID>         <Text>SERÁ QUE PRECISO MOTIVÁ-LOS IGUAL AOS FOSSORIANOS?!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GATE_RAMMING_TIN_1</LocID>           <Text>Quanto mais desesperados ficam, menos chance eles tem de perceberem o plano. Temos que continuar! Ataque as bases deles! </Text>  </GameDBLocalizedString>

             <!--Upon conquering the Firs UNSS base the Fossorians led by Supreme leader Furret blow up the landslide. Everybody expects that they attack our heroes, but they turn the coat and join the players team.--> 
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_FURRET_PART_FURRET_1</LocID>         <Text>3, 2, 1... FOGO NO BURACO! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_FURRET_PART_HOLESTER_1</LocID>       <Text>WOOO-HOOO! Brecha adentro, rapazes!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_FURRET_PART_TIN_1</LocID>            <Text>Os Fossorianos de Furret! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_FURRET_PART_VIKTAR_1</LocID>         <Text>Canalha traidor!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_FURRET_PART_KALI_1</LocID>           <Text>Vi essa chegando a uma légua de distância!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_FURRET_PART_GILEAS_1</LocID>         <Text>Reagrupar!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_FURRET_PART_HOLESTER_2</LocID>       <Text>Não será necessário! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_FURRET_PART_FURRET_2</LocID>         <Text>É hora dos Fossorianos começarem a lutar pelo seu futuro de novo! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_FURRET_PART_RELIC_1</LocID>          <Text>IREI ABRIR O CAMINHO AO SEU PLANETA NATAL, E ASSIM QUE SEUS VELHOS MESTRES LIDAREM COM VOCÊS, FAREI DELES OS MEUS NOVOS ANIMAIS! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_FURRET_PART_FURRET_3</LocID>         <Text>É mesmo?! Então por que esse tempo todo manteve o portal fechado!? Mesmo quando as forças da NUSS precisavam tanto de resuprimento? \n\n...não consegue mais abrir o portal, não é? </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_FURRET_PART_RELIC_2</LocID>          <Text>FURRET, SEU TOLO.   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_FURRET_PART_FURRET_4</LocID>         <Text>Novidades, deusinha! EU SEMPRE FUI TOLO! Tolo o bastante pra arriscar tudo! Esse é o estilo Fossoriano!</Text>  </GameDBLocalizedString>
             
            <!--The second UNSS base is captured, the Relic wants to start orbital bombardment now.-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_TWO_RELIC_1</LocID>        <Text>QUERO QUE ESSE BOMBARDEIO SE INICIE! PARA! ONTEM! DESTRUA OS FOSSORIANOS, TAMBÉM!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_TWO_RAO_1</LocID>          <Text>A evacuação ainda está em andamento! Nosso pessoal ainda está na área! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_TWO_TIN_1</LocID>          <Text>O quê? Dá pra espiar na comunicação NUSS, agora? </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_TWO_FURRET_1</LocID>       <Text>Um presentinho do velho eu... É melhor ter um plano, Princesa. </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_TWO_GILEAS_1</LocID>       <Text>Tenho sim, mas agora é péssima hora pra explicar...</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_TWO_FURRET_2</LocID>       <Text>Não quero os detalhes. Só diga quando e onde, que esse martelo cai!</Text>  </GameDBLocalizedString>

            <!--The third UNSS base is captured - pt1, UNSS fleet is ready to bombard. Princess take control over the fleet by broadcasting the litany through the ship Bright Star on the orbit.-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_RELIC_1</LocID>      <Text>DÊ A ORDEM AGORA, RAO! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_RAO_1</LocID>        <Text>NÃO! Nosso pessoal ainda está lá em baixo!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_RELIC_2</LocID>      <Text>SE QUISER VER OUTRO TRANSPORTE DO SISTEMA SOLAR PASSANDO PELA ESTAÇÃO DO PORTAL DE NOVO, VAI DAR A ORDEM! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_BROWN_1</LocID>      <Text>...lamento, Rao. Não tem outro jeito. \n\nForças terrestres para Frota NUSS. Relatório.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_NIOTA_1</LocID>      <Text>NUSS Rickenbacker, em posição! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_TAKESHI_1</LocID>    <Text>NUSS Ira de Khan, armas prontas! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_MAYA_1</LocID>       <Text>NUSS Normandia, depósito de mísseis destravado! </Text>  </GameDBLocalizedString>
            
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_TIN_1</LocID>        <Text>Chegou a hora! AGORA!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_PRINCESS_1</LocID>   <Text>Estrela Brilhante, está me ouvindo?! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_ANGAEDE_1</LocID>    <Text>SIM, ESTOU PRONTA. TRANSMITIREI A SUA PALAVRA AS NAVES NUSS.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_PRINCESS_2</LocID>   <Text>VAMOS COMEÇAR.</Text>  </GameDBLocalizedString>

            <!--The third UNSS base is captured - pt2 Princes evil sister learns what happened and teleports to orbit although it should not be possible.-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_NIOTA_2</LocID>      <Text>NUSS Rickenbacker para forças terrestres, relatando múltiplos malfuncionamentos no sistema!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_TAKESHI_2</LocID>    <Text>Aqui é a NUSS Normandia, os sistemas não respondem; nossos consoles foram desligados! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_MAYA_2</LocID>       <Text>Tem algo mirando nossas armas na sua posição! Thomas, Rao... [SIGNAL DISTORTION]... FUJAM!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_BROWN_2</LocID>      <Text>Buraco Negro de Nadir! Que diabos está acontecendo?</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_RELIC_3</LocID>      <Text>SOMENTE UMA EXPLICAÇÃO: USARAM A LITANIA. DESCOBRIRAM QUE A IA DAS NAVES NUSS VEM DA MESMA FONTE QUE AS NAVES DOS FILHOS. PRECISO IR A ÓRBITA IMEDIATAMENTE!  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_RELIC_4</LocID>      <Text>TEM UMA FORTE BAGUNÇA NA PSICO-ESFERA, LÁ EM CIMA! NÃO CONSIGO VER DIREITO... \n\nESPERA, ESTOU VENDO! ...UM NOVO BURACO!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_PRINCESS_3</LocID>   <Text>IMPOSSÍVEL! Como?! Ela já se transportou para a órbita! Não era pra ser possível! \n\nEstrela Brilhante, está me ouvindo? ESTRELHA BRILHANTE!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_ANGAEDE_2</LocID>    <Text>...</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_UNSS_BASE_THREE_PRINCESS_4</LocID>   <Text>MERDA! Preciso ir até a nave!  </Text>  </GameDBLocalizedString>

            <!--Princess realises thet there is not enough time to reach the orbit and tries to teleport as well - she appears in orbit though on wrong place. She teleports again, this time on the right spot. -->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_EMERGENCY_TELEP_PRINCESS_1</LocID>   <Text>Não vou conseguir chegar a tempo! Terei que fazer o transporte da mesma forma que a minha irmã! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_EMERGENCY_TELEP_TIN_1</LocID>        <Text>Não, é arriscado demais! Você nem nunca tentou!  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_EMERGENCY_TELEP_PRINCESS_2</LocID>   <Text>Se ela tomar o controle da Frota NUSS, a gente morre de qualquer jeito!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_EMERGENCY_TELEP_PRINCESS_3</LocID>   <Text>Quase acabei dentro desse ferro velho! Ok, mais uma vez!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_EMERGENCY_TELEP_PRINCESS_4</LocID>   <Text>Acho que tô levando jeito pra isso! </Text>  </GameDBLocalizedString>
            
            <!--Princess is coming to ship Bright Star, in the ships shadow the Princess evil sister awaits with Annika brainwashed into serving UNSS. --> 
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_1_RELIC_1</LocID>                <Text>SABIA QUE VIRIA AQUI, IRMÃZINHA. MAS NÃO LUTAREI COM VOCÊ.   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_1_PRINCESS_1</LocID>             <Text>Com medinho, é?  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_1_RELIC_2</LocID>                <Text>NÃO. SIMPLESMENTE NÃO PRECISO. CRIEI UMA FERRAMENTA PARA ESTA OCASIÃO ESPECIAL.    </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_1_PRINCESS_2</LocID>             <Text>Annika!?  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_1_RELIC_3</LocID>                <Text>O QUE RESTOU DELA.   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_1_PRINCESS_3</LocID>             <Text>Quais são suas ordens?   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_1_RELIC_4</LocID>                <Text>MATE-A! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_1_ANNIKA_1</LocID>               <Text>Sim, senhora.   </Text>  </GameDBLocalizedString>
            
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_2_PRINCESS_1</LocID>             <Text>Annika! Não escute ela! Estou aqui com o Lata! Não quero ter que te ferir! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_2_RELIC_1</LocID>                <Text>SABIA QUE HESITARIA AO ENFRENTAR ESSE DEJETO, IRMÃ! VEJO QUE ESCOLHI A FERRAMENTA CERTA. </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_2_PRINCESS_2</LocID>             <Text>Enfrenta isso, Annika! Dá pra se libertar!   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_2_RELIC_2</LocID>                <Text>ESTÁ GASTANDO SALIVA A TOA! DESTRUÍ A INSOLÊNCIA DELA HÁ MUITO TEMPO. ELA ESTÁ SOB O MEU TOTAL CONTROLE. </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_2_PRINCESS_3</LocID>             <Text>Vai pagar pelo o que fez a ela! </Text>  </GameDBLocalizedString>
            
              <!-- Annika is defeated and captured by Princess.-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_3_PRINCESS_1</LocID>             <Text>Peguem ela! Annika não deve ser ferida! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_3_ANNIKA_1</LocID>               <Text>Tirem as mãos de mim! Me deixem ir! </Text>  </GameDBLocalizedString>
            
               <!--Princess talks to her sister and learning important information before the final fight.--> 
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_4_RELIC_1</LocID>                <Text>QUE INFELICIDADE. DEVERIA TER PERCEBIDO QUE DERROTAR VOCÊ ESTÁ ALÉM DA CAPACIDADE DE QUALQUER UM... EXCETO EU! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_4_PRINCESS_1</LocID>             <Text>Então tá esperando o quê!?   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_4_RELIC_2</LocID>                <Text>ESPERAVA MESMO UMA LUTA ÉPICA ENTRE DUAS DEUSAS ESPACIAIS? TEREI QUE DECEPCIONÁ-LA, IRMÃ... NENHUMA DE NÓS PODE USAR OS PODERES.  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_4_PRINCESS_2</LocID>             <Text>Que tipo de truque é esse?!   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_4_RELIC_3</LocID>                <Text>NÃO É TRUQUE! SE LUTARMOS, AS OUTRAS IRÃO OUVIR.   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_4_PRINCESS_3</LocID>             <Text>Que diabos está falando?   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_4_RELIC_4</LocID>                <Text>DE NOSSAS IRMÃS, DE OUTROS SISTEMAS ESTELARES! ASSIM QUE A LUTA COMEÇAR, NÃO CONSEGUIREMOS NOS RESGUARDAR! E A NOSSA ESPÉCIE IRÁ OUVIR!   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_4_PRINCESS_4</LocID>             <Text>Mentirosa! Tá tentando me destrair antes da luta!  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_4_RELIC_5</LocID>                <Text>POR QUE ACHA QUE PAREI DE USAR A ESTAÇÃO DO PORTAL, OU MATEI AS OUTRAS IRMÃS!? PRA FAZER ESSE SISTEMA INVISÍVEL, DE NOVO! MAS DAÍ VOCÊ VEIO E COMEÇOU A FAZER O CONTRÁRIO!   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_4_PRINCESS_5</LocID>             <Text>Não acredito em você. </Text>  </GameDBLocalizedString>
            
               <!--Princess won, but her sister decides to use her psionic powers although this will attract the unwanted attention of the other Allmothers clones in the different solar systems.-->  
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_5_PRINCESS_1</LocID>             <Text>Você perdeu!  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_5_RELIC_1</LocID>                <Text>SÓ O MEU EXÉRCITO. MAS A MÃE-DE-TODOS DENTRO DE MIM PREFERIRIA TRAZER TODO O UNIVERSO AQUI DO QUE ENTREGAR O SISTEMA A VOCÊ! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_5_RELIC_2</LocID>                <Text>MORRA!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_5_PRINCESS_2</LocID>             <Text>Que assim seja!</Text>  </GameDBLocalizedString>
            
               <!-- It's over, Tin comes to the Princess acknowledging her victory. She shared information about the possible future event. End cutscene.-->  
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_TIN_1</LocID>                    <Text>Você conseguiu! Venceu! A gente... QUÊ?! Essa é a Annika? \n\n...consegue me ouvir, Estrelinha?  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_PRINCESS_1</LocID>               <Text>Sim, era pra ela ter sido a última tentativa da minha irmã de me derrotar sem usar os poderes dela. Ela queria esconder esse sistema do resto da minha espécie.   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_TIN_2</LocID>                    <Text>Mas você usou os seus poderes! Eu vi a explosão! Eu... eu não estou entendendo!   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_PRINCESS_2</LocID>               <Text>Vou explicar tudo. Mas agora estou preocupada.  Minha vitória vai nos trazer somente um breve momento de paz. Se a minha espécie vier, teremos que estar prontos. Vamos precisar deste sistema unido.   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_TIN_3</LocID>                    <Text>Está pensando em aceitar o seu legado? Abraçar a Mãe-de-Todos em você!?   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_END_PRINCESS_3</LocID>               <Text>...não.  Agora não. \n\nMas receio que em breve não reste alternativa.</Text>  </GameDBLocalizedString>
            
      <!--      BEFORE/AFTER FIGHT QUOTES-->
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_MERCS_FREDDIE_2</LocID>              <Text>Você quebrou o Alfa! A maioria das partes dele eram por encomenda! \n\nO bônus que vou receber da sua cabeça vai pagar os consertos! </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_MERCS_ALFA_3</LocID>                 <Text>Ai ai ai! Freddie, espera! Estou vindo te ajudar, amigo!</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_MERCS_RH_1</LocID>                   <Text>...e outro comeu poeira.</Text>  </GameDBLocalizedString>
            
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GAHIJI_AF_GAHIJI_1</LocID>           <Text>Estamos sendo derrubados! Mire a artilharia na minha posição!  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GAHIJI_AF_BROWN_1</LocID>            <Text>Como quiser. No três.   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GAHIJI_AF_RAO_1</LocID>              <Text>NÃO!  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_GAHIJI_AF_BROWN_2</LocID>            <Text>Tá bom, Capitão Sem Graça.  </Text>  </GameDBLocalizedString>
            
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_ADELA_BF_RAO_1</LocID>               <Text>Adela, tire seus homens daí!  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_ADELA_BF_ADELA_1</LocID>             <Text>É impossível, estamos cercados ... Capitão, eu ... [ESTÁTICA DE RÁDIO]   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_ADELA_BF_RAO_2</LocID>               <Text>Adela ... Adela! ... DROGA!  </Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_ZAGLOBA_BF_ZAGLOBA_1</LocID>         <Text>Zkurwiona robota ... Do cholery! Flanco esquerdo! Reagrupar! ... </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_ZAGLOBA_BF_RAO_1</LocID>             <Text>Sargento, recue!   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_ZAGLOBA_BF_ZAGLOBA_2</LocID>         <Text>Não dá pra ser feito, garoto!   </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_ZAGLOBA_BF_RAO_2</LocID>             <Text>Renda-se, então! Zagloba, isso é uma ordem direta! "Nada de besteira de herói!" Foi o que você disse pra um cadete há muito tempo!  </Text>  </GameDBLocalizedString>
            
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_ZOLZAYA_BF_RAO_1</LocID>             <Text>Zolzaya, recue!  </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MESSAGE_CAMPAIGN_CRUSADERS_LVL4_ZOLZAYA_BF_ZOLZAYA_1</LocID>         <Text>Estamos ... Capitão! ... [ESTÁTICA DE RÁDIO]  </Text>  </GameDBLocalizedString>

        </LocalizedStrings>
    </GameDBStringTable>
